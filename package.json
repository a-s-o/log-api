{
  "name": "log-api",
  "version": "1.0.0",
  "description": "Sample log api",
  "main": "src/start.js",
  "scripts": {
    "start": "NODE_ENV=production node src/start.js | bunyan -l info",
    "watch": "node-dev src/start.js | bunyan -l info",
    "setup": "node src/setup.js",
    "postinstall": "npm run setup",
    "test": "NODE_ENV=test gulp test",
    "unit": "NODE_ENV=test gulp unit",
    "e2e": "NODE_ENV=test gulp e2e",
    "coverage": "NODE_ENV=test gulp coverage",
    "stop": "docker stop log-api-postgres log-api-kafka log-api-zookeeper; exit 0",
    "destroy": "docker rm -f log-api-postgres log-api-kafka log-api-zookeeper; exit 0"
  },
  "author": "Amandeep",
  "license": "UNLICENSED",
  "dependencies": {
    "@aso/architect": "a-s-o/architect",
    "@aso/bluebird": "a-s-o/bluebird",
    "@aso/tcomb": "a-s-o/tcomb",
    "baconjs": "^0.7.79",
    "bunyan": "^1.5.1",
    "dockerode": "^2.2.3",
    "http-errors": "^1.3.1",
    "joi": "^6.9.1",
    "kafka-node": "^0.2.29",
    "koa": "^1.1.1",
    "koa-body": "^1.3.0",
    "koa-route": "^2.4.2",
    "lodash": "^3.10.1",
    "node-uuid": "^1.4.3",
    "pg": "^4.4.3",
    "pg-hstore": "^2.3.2",
    "reverse-line-reader": "^0.2.6",
    "sequelize": "^3.13.0",
    "sequelize-json": "^2.1.1",
    "shelljs": "^0.5.3"
  },
  "devDependencies": {
    "co-mocha": "^1.1.2",
    "co-supertest": "0.0.10",
    "gulp": "^3.9.0",
    "gulp-istanbul": "^0.10.2",
    "gulp-load-plugins": "^1.0.0",
    "gulp-mocha": "^2.1.3",
    "gulp-plumber": "^1.0.1",
    "isparta": "^3.1.0",
    "mocha": "^2.3.3",
    "node-dev": "^2.7.1",
    "should": "^7.1.1",
    "sinon": "^1.17.2",
    "supertest": "^1.1.0"
  }
}
